<div class="panel panel-primary">
<div class="panel-heading">
<h4 class="text-center">Censored Words List</h4>
</div>
<form action="<tag:frm_action />" method="post" enctype="multipart/form-data">
  <table class="lista" width="100%" align="center">
    <tr>
      <td align="center"><p class="text-warning"><tag:language.CENSORED_NOTE /></p></td>
    </tr>
    <tr>
      <td align="center"><textarea name="badwords" rows="20" cols="60"><tag:censored_text /></textarea></td>
    </tr>
    <tr>
      <td align="center">
        <input type="submit" name="write" class="btn btn-success" value="<tag:language.FRM_CONFIRM />" />&nbsp;&nbsp;
        <input type="submit" name="write" class="btn btn-warning" value="<tag:language.FRM_CANCEL />" />
      </td>
    </tr>
  </table>
</form>
<div class="panel-footer">
</div>
</div>
